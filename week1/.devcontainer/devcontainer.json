{
  "name": "Flutter Web Dev",
  "image": "mcr.microsoft.com/devcontainers/dotnet:6.0",  // base image
  "features": {
    "ghcr.io/devcontainers/features/flutter:1": {}
  },
  "postCreateCommand": "flutter config --enable-web && flutter pub get",
  "customizations": {
    "vscode": {
      "extensions": [
        "Dart-Code.dart-code",
        "Dart-Code.flutter"
      ]
    }
  }
}
